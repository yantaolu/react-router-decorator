{
  "name": "react-router-decorator",
  "version": "0.1.4",
  "description": "基于 react-router-dom 封装的路由工具，提供装饰器/函数模式设置路由，自动排序，支持嵌套路由",
  "files": [
    "lib"
  ],
  "module": "lib/index.mjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev:example": "tsup --watch --config ./tsup.example.ts",
    "dev": "tsup --watch",
    "build": "tsup",
    "prepare": "husky install",
    "prepublish": "node ./scripts/prev-publish.js && npm run build",
    "postpublish": "node ./scripts/post-publish.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react-router-dom",
    "react-router",
    "decorator-router",
    "decorator"
  ],
  "repository": "https://github.com/yantaolu/react-router-decorator",
  "homepage": "https://github.com/yantaolu/react-router-decorator#readme",
  "dependencies": {
    "react-router-dom": "^6.11.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@swc/core": "^1.3.58",
    "@types/node": "14",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "chalk": "^4.1.2",
    "commitizen": "^4.3.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.1.2",
    "git-cz": "^4.9.0",
    "husky": "^8.0.0",
    "inquirer": "^8.2.5",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^15.6.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "author": "YantaoLu",
  "license": "ISC",
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,less,sass,scss,css,json,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
